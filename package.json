{
  "name": "xavier-syntax",
  "displayName": "Xavier Syntax",
  "publisher": "Devmachinist",
  "author": "Matthew Teague",
  "description": "A Xavier Syntax helper in a VS Code extension",
  "version": "0.0.2",
  "repository": "https://github.com/lxNewsxl/Xavier-Syntax.git",
  "icon": "assets/Logo.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "xavier",
      "aliases": ["Xavier", "xavier"],
      "extensions": [".xavier"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "xavier",
      "scopeName": "source.xavier",
      "path": "./syntaxes/xavier.tmLanguage.json"

    }]
  }
}